<UserControl x:Class="SequenceEditor.ConfigViewer"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<DockPanel Name="pnlMain">
		<ToolBar DockPanel.Dock="Top" Name="toolBtns" ToolBarTray.IsLocked="True">
			<Button Content="Flip Seq" Name="btnFlipSeq" Click="BtnFlipSeq_Click"></Button>
			<Button Content="Toggle Grid" Name="btnToGrid" Click="BtnToGrid_Click"></Button>
			<Separator></Separator>
			<Button Content="Collapse All" Name="btnCollapse" Click="BtnCollapse_Click"></Button>
			<Button Content="Expand All" Name="btnExpand" Click="BtnExpand_Click"></Button>
			<Separator></Separator>
			<Button Content="Save" Name="btnSave" Click="BtnSave_Click"></Button>
			<Button Content="Revert Changes" Name="btnRevert" Click="BtnRevert_Click"></Button>
		</ToolBar>
	</DockPanel>
</UserControl>